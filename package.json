    "redux-logger": "^3.0.6",
    "slick-carousel": "^1.8.1",
    "typescript": "^4.4.2",
    "use-react-router-breadcrumbs": "^3.2.1",
    "web-vitals": "^2.1.0",
    "yup": "^0.32.11"
